{
	"app": {
		"debug": true,
		"timezone": "UTC",
		"url": "0.0.0.0",
		"port": 1337,
		"version": "0.4.0"
	},

	"camera": {
		"command": "imagesnap stream/image.jpg",
		"interval": 2000
	},

	"channels": {
		"dashboard": {
			"gcode": [ "code" ],
			"home": [],
			"jog": [ "axis", "dist" ],
			"start": [ "hash", "printjobID" ],
			"pause": [],
			"resume": [],
			"stop": [],
			"temp_extruder": [ "extnr", "temp" ],
			"temp_bed": [ "temp" ],
			"lcd_message": [ "msg" ],
			"extrude": [ "extnr", "dist" ],
			"retract": [ "extnr", "dist" ],
			"fan": [ "fannr", "speed" ],
			"reconnect": [],
			"autolevel": [],
			"door_open": [],
			"door_close": []
		},

		"driver": {
			"status": [ "printjobID", "status", "progress", "extruders", "bed" ],
			"finished": [ "printjobID" ],
			"connect": [],
			"disconnect": [],
			"error": [ "msg" ],
			"notification": [ "msg" ]
		}
	},

	"cloud": {
		"url": "https://ws.formide.com"
	},

	"dashboard": {
		"port": 8080,
		"path": "../formideOS-interface",
		"auto": false
	},

	"database": {
		"storage": "../storage/database"
	},

	"log": {
		"path": "../storage"
	},

	"paths": {
		"temp": "../uploads/tmp",
		"modelfile": "../uploads/modelfiles",
		"gcode": "../uploads/gcode"
	},

	"printer": {
		"port": 1338,
		"simulated": false
	},

	"slicer": {
		"port": 1339,
		"simulated": false
	},

	"settings": {
		"path": "../storage"
	}
}